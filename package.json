  "name": "nodeswork",
  "description": "Nodeswork is a framework to build simple applications and to deploy to user's devices.",
  "version": "0.0.13",
  "repository": {
    "type": "git",
    "url": "git@github.com:nodeswork/nodeswork.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {

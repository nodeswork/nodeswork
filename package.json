    "koa-router": "^7.0.0",
    "mongoose": "^4.9.4",
    "nodeswork-mongoose": "^0.0.2",
    "nodeswork-utils": "^0.0.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "underscore": "^1.8.3",
    "winston-mongodb": "^2.0.10"
  },
